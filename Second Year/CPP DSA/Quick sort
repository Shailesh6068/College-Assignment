    #include <iostream>  
    using namespace std;
    int partition (int a[], int start, int end)  
    {  
        int pivot = a[end]; 
        int i = (start - 1);  
        cout<<"Pivot :"<<pivot<<endl;
        for (int j = start; j <= end - 1; j++)  
        {  
            if (a[j] < pivot)  
            {  
                i++; 
                int t = a[i];  
                a[i] = a[j];  
                a[j] = t;  
                cout<<"temp :"<<t<<"  a[i] :"<<a[i]<<"   a[j] :"<<a[j]<<endl;
            }  
        }  
        int t = a[i+1];  
        a[i+1] = a[end];  
        a[end] = t;  
        cout<<" AA= t :"<<t<<"  a :"<<a[i]<<"  a :"<<a[end]<<endl;
        return (i + 1);  
    }  
      
    
    void quick(int a[], int start, int end) 
    {  
        if (start < end)  
        {  
            int p = partition(a, start, end);   
            quick(a, start, p - 1);  
            quick(a, p + 1, end);  
        }  
    }  
      
    /* function to print an array */  
    void printArr(int a[], int n)  
    {  
        int i;  
        for (i = 0; i < n; i++)  
            printf("%d ", a[i]);  
        
        cout<<endl;
    }  
    int main()  
    {  
        int a[] = { 24, 9, 29, 14};  
        int n = sizeof(a) / sizeof(a[0]);  
        printf("Before sorting array elements are - \n");  
        printArr(a, n);  
        quick(a, 0, n - 1);  
        printf("\nAfter sorting array elements are - \n");    
        printArr(a, n);  
          
        return 0;  
    }  
